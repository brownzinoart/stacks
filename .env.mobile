# Mobile-specific environment variables
# Copy this to .env.local when building for mobile

# API URLs for mobile
# Replace with your production API URL when deploying
NEXT_PUBLIC_API_URL=https://api.stacks-library.com
NEXT_PUBLIC_BACKEND_URL=https://backend.stacks-library.com

# Supabase (same as web)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Mobile-specific features
NEXT_PUBLIC_IS_MOBILE=true
NEXT_PUBLIC_ENABLE_AR=true
NEXT_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true

# External services (mobile API keys if different)
NEXT_PUBLIC_MAPBOX_TOKEN=your_mapbox_token